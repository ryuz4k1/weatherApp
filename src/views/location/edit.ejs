<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../partials/head %>
</head>
<body class="hold-transition skin-yellow-light sidebar-mini">
    <div class="wrapper">
        <% include ../partials/header %>
        
        <% include ../partials/sidebar %>

        <div class="content-wrapper">
        
            <!-- Main content -->
            <section class="content">
                <!-- Default box -->
                <form method="POST" action="/location/edit">
                <input type="hidden" name="locationId" value="<%= result.locationId%>">

                <div class="box">
                    <div class="box-header with-border">
                        <h3 class="box-title">Edit Location</h3>
                    </div>
                    <div class="box-body">
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label>Address</label>
                                    <input type="text" class="form-control" id="address" name="address" value="<%= result.address%>" required>
                                </div>

                                <div class="form-group">
                                    <label>adminDistrict</label>
                                    <input type="text" class="form-control" id="adminDistrict" name="adminDistrict" value="<%= result.adminDistrict%>" required>
                                </div>

                                <div class="form-group">
                                    <label>adminDistrict</label>
                                    <input type="text" class="form-control" id="adminDistrict" name="adminDistrict" value="<%= result.adminDistrict%>" required>
                                </div>


                                <div class="form-group">
                                    <label>city</label>
                                    <input type="text" class="form-control" id="adminDistrict" name="adminDistrict" value="<%= result.city%>" required>
                                </div>


                                <div class="form-group">
                                    <label>country</label>
                                    <input type="text" class="form-control" id="adminDistrict" name="adminDistrict" value="<%= result.country%>" required>
                                </div>


                                <div class="form-group">
                                    <label>countryCode</label>
                                    <input type="text" class="form-control" id="adminDistrict" name="adminDistrict" value="<%= result.countryCode%>" required>
                                </div>

                                <div class="form-group">
                                    <label>displayName</label>
                                    <input type="text" class="form-control" id="adminDistrict" name="adminDistrict" value="<%= result.displayName%>" required>
                                </div>

                                <div class="form-group">
                                    <label>latitude</label>
                                    <input type="text" class="form-control" id="adminDistrict" name="adminDistrict" value="<%= result.latitude%>" required>
                                </div>

                                <div class="form-group">
                                    <label>neighborhood</label>
                                    <input type="text" class="form-control" id="adminDistrict" name="adminDistrict" value="<%= result.neighborhood%>" required>
                                </div>

                                <div class="form-group">
                                    <label>postalCode</label>
                                    <input type="text" class="form-control" id="adminDistrict" name="adminDistrict" value="<%= result.postalCode%>" required>
                                </div>

                                <div class="form-group">
                                    <label>placeId</label>
                                    <input type="text" class="form-control" id="adminDistrict" name="adminDistrict" value="<%= result.placeId%>" required>
                                </div>
                            </div>


                            <div class="col-md-6">
                                <div class="form-group">
                                    <label>adminDistrictCode</label>
                                    <input type="text" class="form-control" id="address" name="address" value="<%= result.adminDistrictCode%>" required>
                                </div>

                                <div class="form-group">
                                    <label>disputedArea</label>
                                    <input type="text" class="form-control" id="adminDistrict" name="adminDistrict" value="<%= result.disputedArea%>" required>
                                </div>

                                <div class="form-group">
                                    <label>ianaTimeZone</label>
                                    <input type="text" class="form-control" id="address" name="address" value="<%= result.ianaTimeZone%>" required>
                                </div>

                                <div class="form-group">
                                    <label>icaoCode</label>
                                    <input type="text" class="form-control" id="adminDistrict" name="adminDistrict" value="<%= result.icaoCode%>" required>
                                </div>

                                <div class="form-group">
                                    <label>locationCategory</label>
                                    <input type="text" class="form-control" id="address" name="address" value="<%= result.locationCategory%>" required>
                                </div>

                                <div class="form-group">
                                    <label>locId</label>
                                    <input type="text" class="form-control" id="adminDistrict" name="adminDistrict" value="<%= result.locId%>" required>
                                </div>

                                <div class="form-group">
                                    <label>longitude</label>
                                    <input type="text" class="form-control" id="adminDistrict" name="adminDistrict" value="<%= result.longitude%>" required>
                                </div>

                                <div class="form-group">
                                    <label>pwsId</label>
                                    <input type="text" class="form-control" id="adminDistrict" name="adminDistrict" value="<%= result.pwsId%>" required>
                                </div>

                                <div class="form-group">
                                    <label>type</label>
                                    <input type="text" class="form-control" id="adminDistrict" name="adminDistrict" value="<%= result.type%>" required>
                                </div>

                                <div class="form-group">
                                    <label>createdOn</label>
                                    <input type="text" class="form-control" id="adminDistrict" name="adminDistrict" value="<%= result.date%>" required readonly>
                                </div>
                            </div>
    
                          
                        </div>
                    </div>

                    <div class="box-footer">
                        <button type="submit" id="import" class="btn btn-info pull-right">Add</button>
                    </div>
                <!-- /.box -->
                </form>
            </section>
            <!-- /.content -->
        </div>

    </div>
    <!-- page script -->
    <% include ../partials/script %>
    <script>
        $(function () {
            //Initialize Select2 Elements
            $('.select2').select2()
        })
        
    </script>

</body>
</html>